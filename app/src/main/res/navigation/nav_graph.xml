<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/jokeGeneratorFragment">

    <fragment
        android:id="@+id/jokeGeneratorFragment"
        android:name="com.example.jokeapp.presentation.screens.jokeGenerator.JokeGeneratorFragment"
        android:label="JokeGeneratorFragment"
        tools:layout="@layout/fragment_joke_genrator">
        <action
            android:id="@+id/action_jokeGeneratorFragment_to_savedJokesFragment"
            app:destination="@id/savedJokesFragment" />
    </fragment>
    <fragment
        android:id="@+id/savedJokesFragment"
        android:name="com.example.jokeapp.presentation.screens.savedJokes.SavedJokesFragment"
        android:label="SavedJokesFragment"
        tools:layout="@layout/fragment_saved_jokes"/>
</navigation>